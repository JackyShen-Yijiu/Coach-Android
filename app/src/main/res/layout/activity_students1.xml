<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.blackcat.coach.activities.StudentsActivity1">
<!--    xmlns:app="http://schemas.android.com/apk/res-auto/com.jzjf.coach" -->
    <include layout="@layout/layout_toolbar" />

    <com.blackcat.coach.lib.PagerSlidingTab
        android:id="@+id/act_student1_sliding_tab"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:textSize="14sp"
        app:tabPaddingLeftRight="32dp"
        android:textColor="#333"
        app:indicatorColor="@color/slidingtab_indicatorcolor"
        app:underlineColor="#55333333"
        app:underlineHeight="1dp"
        app:indicatorHeight="1dp"
        android:fillViewport="true">
    </com.blackcat.coach.lib.PagerSlidingTab>

    <android.support.v4.view.ViewPager
        android:id="@+id/act_student1_view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </android.support.v4.view.ViewPager>

</LinearLayout>
